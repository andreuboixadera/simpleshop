    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/waitress-0.8.2-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Prova/simpleshop/eggs/Paste-1.7.5.1-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Prova/simpleshop/eggs/setuptools-0.6c12dev_r88846-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/PasteDeploy-1.5.0-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Prova/simpleshop/eggs/nose-1.2.1-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/pyramid_debugtoolbar-1.0.4-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Prova/simpleshop/src/simpleshop',
    '/home/andreu/Documents/PYTHON/Prova/simpleshop/eggs/Pygments-1.6-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/translationstring-1.1-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/venusian-1.0a7-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/zope.deprecation-4.0.2-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/zope.interface-4.0.3-py2.7-linux-i686.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/repoze.lru-0.6-py2.7.egg',
    '/home/andreu/Documents/PYTHON/Botiga/lib/python2.7/site-packages/WebOb-1.2.3-py2.7.egg',
